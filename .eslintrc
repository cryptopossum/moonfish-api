{
    "extends": "airbnb-base",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true,
      "es6": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1,
      "import/prefer-default-export": 0,
      "eol-last": 0,
      "arrow-body-style": 0,
      "arrow-parens": 0
    },
    "plugins": ["jest"]
  }
